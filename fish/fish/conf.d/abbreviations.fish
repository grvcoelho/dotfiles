# Git abbreviations
abbr ga 'git add'
abbr gc 'git commit'
abbr gco 'git checkout'
abbr gp 'git push'
abbr gf 'git fetch'
abbr gl 'git log'
abbr gll 'git log --oneline'

# Tmux abbreviations
abbr ta 'tmux attach -t'
abbr tl 'tmux list-sessions'
abbr ts 'tmux new-session -s'
abbr tkss 'tmux kill-session -t'
