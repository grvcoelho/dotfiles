# Git abbreviations
abbr ga 'git add'
abbr gs 'git status'
abbr gc 'git commit'
abbr gco 'git checkout'
abbr gp 'git push'
abbr gpl 'git pull'
abbr gf 'git fetch'
abbr gl 'git log'
abbr gll 'git log --graph --decorate --pretty=oneline --abbrev-commit'

# Tmux abbreviations
abbr ta 'tmux attach -t'
abbr tl 'tmux list-sessions'
abbr ts 'tmux new-session -s'
abbr tkss 'tmux kill-session -t'
